<h1>Listing restrictions</h1>

<table>
  <tr>
    <th>Optin</th>
    <th>Optout</th>
    <th>Inadimplente</th>
    <th>Funcionario</th>
    <th>Procon</th>
    <th>Aca</th>
    <th>Triplea</th>
    <th>Corp</th>
    <th>Suspenso</th>
    <th>Mailing</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @restrictions.each do |restriction| %>
  <tr>
    <td><%= restriction.optin %></td>
    <td><%= restriction.optout %></td>
    <td><%= restriction.inadimplente %></td>
    <td><%= restriction.funcionario %></td>
    <td><%= restriction.procon %></td>
    <td><%= restriction.aca %></td>
    <td><%= restriction.triplea %></td>
    <td><%= restriction.corp %></td>
    <td><%= restriction.suspenso %></td>
    <td><%= restriction.mailing_id %></td>
    <td><%= link_to 'Show', restriction %></td>
    <td><%= link_to 'Edit', edit_restriction_path(restriction) %></td>
    <td><%= link_to 'Destroy', restriction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Restriction', new_restriction_path %>
